FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      b;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
  inlet
  {
    type          fixedValue;
    value         $internalField;
  }
  outlet
  {
    type          zeroGradient;
  }
  walls
  {
    type          zeroGradient;
  }
  container
  {
    type          zeroGradient;
  }

}

