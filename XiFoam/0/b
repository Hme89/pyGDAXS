FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      b;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{

  "side.*"
  {
    type          inletOutlet;
    inletValue    $internalField;
  }

  ground
  {
    type          zeroGradient;
  }

  "container.*"
  {
    type          zeroGradient;
  }

  base
  {
    type          zeroGradient;
  }

  inlet
  {
    type          fixedValue;
    value         $internalField;
  }

}

