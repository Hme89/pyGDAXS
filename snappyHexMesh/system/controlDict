FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     snappyHexMesh;

startFrom       latestTime;

stopAt          endTime;

endTime         1;

deltaT          1;

writeInterval   1;

writeFormat     binary;

writeCompression off;

timeFormat      general;

runTimeModifiable true;

