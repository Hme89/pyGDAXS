FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         multiComponentMixture;
    transport       const;
    thermo          hConst;
    energy          sensibleEnthalpy;
    equationOfState perfectGas;
    specie          specie;
}

species
(
  H2
  AIR
);

H2
{
  specie
  {
      molWeight       2.016;
  }
  thermodynamics
  {
    Cp                14320;
    Hf                117;
  }
  transport
  {
    mu                  1.37e-5;  // at 300K
    Pr                  0.69;     // at 300K
  }
}

AIR
{
    specie
    {
        molWeight   28.9;
    }
    thermodynamics
    {
      Cp          1005;
      Hf             0;
    }
    transport
    {
        mu          2.98e-5;
        Pr          0.7;
    }
}


