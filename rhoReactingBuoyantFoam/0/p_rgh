FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{

  outlet
  {
    type            fixedFluxPressure;
    // gradient        uniform 0;
    value           $internalField;
  }

  walls
  {
    type            fixedFluxPressure;
    gradient        uniform 0;
    value           $internalField;
    }

  inlet
  {
    type            fixedFluxPressure;
    gradient        uniform 0;
    value           $internalField;
  }


}

