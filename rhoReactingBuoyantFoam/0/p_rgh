FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

// internalField   uniform 101325;
internalField   uniform 0;

boundaryField
{

  "side.*"
  {
    // type            fixedValue;
    // type            zeroGradient;
    type            fixedFluxPressure;
    value           $internalField;
    // type            totalPressure;
    // p0              $internalField;

  }

  ground
  {
    type            fixedFluxPressure;
    value           $internalField;
    }

  container
  {
    type            fixedFluxPressure;
    value           $internalField;
  }

  inlet
  {
    type            fixedFluxPressure;
    value           internalField;
  }

  base
  {
    type            fixedFluxPressure;
    value           $internalField;
  }

}

