FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1.5;
#include        "include/jetCoeffs"

boundaryField
{

  #include      "include/sidesZeroGradient"

  ground
  {
    type        kqRWallFunction;
    value       $internalField;
  }

  container
  {
    type        kqRWallFunction;
    value       $internalField;
  }

  inlet
  {
    type        fixedValue;
    value       $kjet;
  }

  base
  {
    type        zeroGradient;
  }

}

